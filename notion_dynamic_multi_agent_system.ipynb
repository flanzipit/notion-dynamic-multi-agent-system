        "#### Notion API Connector\n",
        "\n",
        "- `query_database`: Fetches data from Notion databases with filtering and sorting\n",
        "- `batch_update`: Performs batch updates to Notion pages with rate limiting\n",
        "- `get_properties`: Automatically fetches and formats all Notion database properties\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "a2xWb8VXMAa0"
      },
      "outputs": [],
      "source": [
        "class NotionConnector:\n",
        "    \"\"\"Handles all Notion API operations\"\"\"\n",
        "\n",
        "    def __init__(self, token: str, database_ids: Dict[str, str]):\n",
        "        self.client = Client(auth=token)\n",
        "        self.database_ids = database_ids\n",
        "        self.rate_limit_delay = 0.4  # Notion allows ~3 requests/second\n",
        "\n",
        "    def _rate_limit(self):\n",
        "        \"\"\"Simple rate limiting\"\"\"\n",
        "        time.sleep(self.rate_limit_delay)\n",
        "\n",
        "    def get_properties(self) -> Dict:\n",
        "        \"\"\"Load all Notion database properties\"\"\"\n",
        "        print(\"ðŸ“¡ NotionConnector: Loading properties...\")\n",
        "        properties = {}\n",
        "\n",
        "        for db_name, db_id in self.database_ids.items():\n",
        "            try:\n",
        "                self._rate_limit()\n",
        "                db_info = self.client.databases.retrieve(database_id=db_id)\n",
        "                props = db_info['properties']\n",
        "                \n",
        "                prop_list = []\n",
        "                for prop_name, prop_data in sorted(props.items()):\n",
        "                    prop_type = prop_data['type']\n",
        "                    prop_str = f\"'{prop_name}' - {prop_type}\"\n",
        "                    \n",
        "                    # Add options for select/multi-select properties\n",
        "                    if prop_type in ['select', 'multi_select'] and prop_data[prop_type].get('options'):\n",
        "                        options = [opt['name'] for opt in prop_data[prop_type]['options']]\n",
        "                        prop_str += f\" | Valid values: {options}\"\n",
        "                    \n",
        "                    prop_list.append(prop_str)\n",
        "\n",
        "                properties[db_name] = prop_list\n",
        "            except Exception as e:\n",
        "                print(f\"âŒ Error loading properties for {db_name}: {str(e)}\")\n",
        "                properties[db_name] = []\n",
        "\n",
        "        print(f\"âœ… NotionConnector: Loaded properties for {len(properties)} databases\")\n",
        "        return properties\n",
        "\n",
        "    def query_database(self, database_id: str, filters: Dict = None, sorts: List = None, limit: int = 100) -> List[Dict]:\n",
        "        \"\"\"Query Notion database with optional filters and sorts\"\"\"\n",
        "        db_name = next((name for name, id in self.database_ids.items() if id == database_id), database_id)\n",
        "        print(f\"ðŸ“¡ NotionConnector: Querying {db_name} database...\")\n",
        "        \n",
        "        all_results = []\n",
        "        has_more = True\n",
        "        next_cursor = None\n",
        "        \n",
        "        try:\n",
        "            while has_more and len(all_results) < limit:\n",
        "                self._rate_limit()\n",
        "                \n",
        "                query_params = {\n",
        "                    'database_id': database_id,\n",
        "                    'page_size': min(100, limit - len(all_results))\n",
        "                }\n",
        "                \n",
        "                if filters:\n",
        "                    query_params['filter'] = filters\n",
        "                if sorts:\n",
        "                    query_params['sorts'] = sorts\n",
        "                if next_cursor:\n",
        "                    query_params['start_cursor'] = next_cursor\n",
        "                \n",
        "                response = self.client.databases.query(**query_params)\n",
        "                \n",
        "                # Transform results to more usable format\n",
        "                for page in response['results']:\n",
        "                    transformed_page = self._transform_page(page)\n",
        "                    all_results.append(transformed_page)\n",
        "                \n",
        "                has_more = response.get('has_more', False)\n",
        "                next_cursor = response.get('next_cursor')\n",
        "                \n",
        "        except Exception as e:\n",
        "            print(f\"âŒ Error querying {db_name}: {str(e)}\")\n",
        "            return []\n",
        "        \n",
        "        print(f\"âœ… NotionConnector: Loaded {len(all_results)} {db_name} records\")\n",
        "        return all_results\n",
        "\n",
        "    def _transform_page(self, page: Dict) -> Dict:\n",
        "        \"\"\"Transform Notion page to simplified format\"\"\"\n",
        "        result = {\n",
        "            'id': page['id'],\n",
        "            'created_time': page['created_time'],\n",
        "            'last_edited_time': page['last_edited_time'],\n",
        "            'properties': {}\n",
        "        }\n",
        "        \n",
        "        for prop_name, prop_data in page['properties'].items():\n",
        "            prop_type = prop_data['type']\n",
        "            \n",
        "            if prop_type == 'title':\n",
        "                result['properties'][prop_name] = prop_data['title'][0]['text']['content'] if prop_data['title'] else ''\n",
        "            elif prop_type == 'rich_text':\n",
        "                result['properties'][prop_name] = prop_data['rich_text'][0]['text']['content'] if prop_data['rich_text'] else ''\n",
        "            elif prop_type == 'number':\n",
        "                result['properties'][prop_name] = prop_data['number']\n",
        "            elif prop_type == 'select':\n",
        "                result['properties'][prop_name] = prop_data['select']['name'] if prop_data['select'] else None\n",
        "            elif prop_type == 'multi_select':\n",
        "                result['properties'][prop_name] = [item['name'] for item in prop_data['multi_select']]\n",
        "            elif prop_type == 'date':\n",
        "                result['properties'][prop_name] = prop_data['date']['start'] if prop_data['date'] else None\n",
        "            elif prop_type == 'email':\n",
        "                result['properties'][prop_name] = prop_data['email']\n",
        "            elif prop_type == 'phone_number':\n",
        "                result['properties'][prop_name] = prop_data['phone_number']\n",
        "            elif prop_type == 'url':\n",
        "                result['properties'][prop_name] = prop_data['url']\n",
        "            elif prop_type == 'relation':\n",
        "                result['properties'][prop_name] = [rel['id'] for rel in prop_data['relation']]\n",
        "            else:\n",
        "                result['properties'][prop_name] = str(prop_data.get(prop_type, ''))\n",
        "        \n",
        "        return result\n",
        "\n",
        "    def batch_update(self, updates: Dict) -> None:\n",
        "        \"\"\"Perform batch updates to Notion pages\"\"\"\n",
        "        for db_name, update_list in updates.items():\n",
        "            if not update_list:\n",
        "                continue\n",
        "\n",
        "            print(f\"ðŸ“¡ NotionConnector: Updating {len(update_list)} {db_name} pages...\")\n",
        "\n",
        "            # Notion doesn't have native batch API, so we process sequentially with rate limiting\n",
        "            success_count = 0\n",
        "            for update in update_list:\n",
        "                try:\n",
        "                    self._rate_limit()\n",
        "                    \n",
        "                    page_id = update['id']\n",
        "                    properties = update['properties']\n",
        "                    \n",
        "                    # Convert properties to Notion format\n",
        "                    notion_properties = self._convert_properties_for_update(properties)\n",
        "                    \n",
        "                    self.client.pages.update(\n",
        "                        page_id=page_id,\n",
        "                        properties=notion_properties\n",
        "                    )\n",
        "                    \n",
        "                    success_count += 1\n",
        "                    \n",
        "                except Exception as e:\n",
        "                    print(f\"âŒ Failed to update page {update.get('id', 'unknown')}: {str(e)}\")\n",
        "                    continue\n",
        "\n",
        "            print(f\"âœ… NotionConnector: {success_count}/{len(update_list)} {db_name} updates completed\")\n",
        "\n",
        "    def _convert_properties_for_update(self, properties: Dict) -> Dict:\n",
        "        \"\"\"Convert simple properties to Notion API format\"\"\"\n",
        "        notion_props = {}\n",
        "        \n",
        "        for prop_name, value in properties.items():\n",
        "            if isinstance(value, str):\n",
        "                if prop_name.lower() in ['name', 'title']:\n",
        "                    notion_props[prop_name] = {\n",
        "                        'title': [{'text': {'content': value}}]\n",
        "                    }\n",
        "                elif '@' in value:  # Email\n",
        "                    notion_props[prop_name] = {'email': value}\n",
        "                elif value.startswith('http'):  # URL\n",
        "                    notion_props[prop_name] = {'url': value}\n",
        "                else:  # Rich text or select\n",
        "                    # Try as select first, fallback to rich text\n",
        "                    notion_props[prop_name] = {'select': {'name': value}}\n",
        "            elif isinstance(value, (int, float)):\n",
        "                notion_props[prop_name] = {'number': value}\n",
        "            elif isinstance(value, list):\n",
        "                notion_props[prop_name] = {\n",
        "                    'multi_select': [{'name': str(item)} for item in value]\n",
        "                }\n",
        "        \n",
        "        return notion_props\n",
        "\n",
        "    def get_data(self, database_name: str) -> List[Dict]:\n",
        "        \"\"\"Get all data from a specific database\"\"\"\n",
        "        if database_name not in self.database_ids:\n",
        "            print(f\"âŒ Database '{database_name}' not found in configured databases\")\n",
        "            return []\n",
        "        \n",
        "        return self.query_database(self.database_ids[database_name])"
      ]
    },
